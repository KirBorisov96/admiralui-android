<?xml version="1.0" encoding="utf-8"?>
<com.admiral.uikit.components.tabs.LogoTabs xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/tabsThree"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:checkedId="@+id/one">

    <com.admiral.uikit.components.tabs.LogoTab
        android:id="@+id/one"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        app:srcCompat="@drawable/test_ic_visa" />

    <com.admiral.uikit.components.tabs.LogoTab
        android:id="@+id/two"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        app:srcCompat="@drawable/test_ic_master_card" />

    <com.admiral.uikit.components.tabs.LogoTab
        android:id="@+id/three"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        app:srcCompat="@drawable/test_ic_mir" />

</com.admiral.uikit.components.tabs.LogoTabs>