<?xml version="1.0" encoding="utf-8"?>
<com.admiral.uikit.components.tabs.IconTabs xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/tabsThree"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:checkedId="@+id/one">

    <com.admiral.uikit.components.tabs.IconTab
        android:id="@+id/one"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        app:admiralIcon="@drawable/admiral_ic_mobile_solid"
        app:admiralText="@string/tabs_one" />

    <com.admiral.uikit.components.tabs.IconTab
        android:id="@+id/two"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        app:admiralIcon="@drawable/admiral_ic_card_solid"
        app:admiralText="@string/tabs_two" />

    <com.admiral.uikit.components.tabs.IconTab
        android:id="@+id/three"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        app:admiralIcon="@drawable/admiral_ic_card_solid"
        app:admiralText="@string/tabs_three" />

</com.admiral.uikit.components.tabs.IconTabs>